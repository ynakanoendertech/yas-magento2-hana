<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no"/>

        <!-- favicon -->
        <link rel="apple-touch-icon" sizes="180x180" src="Magento_Theme::apple-touch-icon.png" />
        <link rel="icon" type="image/png" sizes="32x32" src="Magento_Theme::favicon-32x32.png" />
        <link rel="icon" type="image/png" sizes="16x16" src="Magento_Theme::favicon-16x16.png" />
        <link rel="manifest" src="Magento_Theme::manifest.json" />
        <link rel="mask-icon" src="Magento_Theme::safari-pinned-tab.svg" color="#5bbad5" />
        <meta name="theme-color" content="#ffffff" />

        <!-- Google Fonts -->
        <link src="https://fonts.googleapis.com/css?family=Satisfy" rel="stylesheet" type="text/css" src_type="url" />
        <link src="https://fonts.googleapis.com/css?family=Noto+Serif:400,700" rel="stylesheet" type="text/css" src_type="url" />
        <link src="https://fonts.googleapis.com/css?family=Overpass:400,400i,600,600i,700,700i,800,800i" rel="stylesheet" type="text/css" src_type="url" />
    </head>
</page>
